{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@nx-post/api/data-access-comment": [
        "libs/api/data-access-comment/src/index.ts"
      ],
      "@nx-post/api/data-access-post": [
        "libs/api/data-access-post/src/index.ts"
      ],
      "@nx-post/api/data-access-user": [
        "libs/api/data-access-user/src/index.ts"
      ],
      "@nx-post/api/feature-comment": ["libs/api/feature-comment/src/index.ts"],
      "@nx-post/api/feature-config": ["libs/api/feature-config/src/index.ts"],
      "@nx-post/api/feature-post": ["libs/api/feature-post/src/index.ts"],
      "@nx-post/api/feature-user": ["libs/api/feature-user/src/index.ts"],
      "@nx-post/api/shared-data-access-dtos": [
        "libs/api/shared-data-access-dtos/src/index.ts"
      ],
      "@nx-post/api/shared-data-access-entities": [
        "libs/api/shared-data-access-entities/src/index.ts"
      ],
      "@nx-post/api/shared-data-access-mappings": [
        "libs/api/shared-data-access-mappings/src/index.ts"
      ],
      "@nx-post/api/utils-config": ["libs/api/utils-config/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
